{
    "version": "3.0",

    "appDir": "./",
    "assetsDir": "./",

    "libs": {
        "ria": "../../../../src/framework/"
    },

    "options": {
        "uglifyjs": {
            "mangle": false,
            "mangle_options_": {},
            "squeeze": false,
            "squeeze_options_": {
                "make_seqs": true,
                "dead_code": true,
                "no_warnings": false,
                "extra": false
            },
            "beautify": true
        }
    },

    "modules": [{
        "name": "class_test",
        "in": "./class_test.js",
        "out": "./class_test.compiled.js"
    },{
        "name": "ctor_test",
        "in": "./ctor_test.js",
        "out": "./ctor_test.compiled.js"
    },{
        "name": "interface_test",
        "in": "./interface_test.js",
        "out": "./interface_test.compiled.js"
    },{
        "name": "method_test",
        "in": "./method_test.js",
        "out": "./method_test.compiled.js"
    },{
        "name": "property_test",
        "in": "./property_test.js",
        "out": "./property_test.compiled.js"
    }]
}