{
  "appDir": "app",

  "options": {
    "uglifyjs": {
      "mangle": false,
      "mangle_toplevel": false,
      "squeeze": true,
      "squeeze_options": {
        "make_seqs": true,
        "dead_code": true,
        "no_warnings": false,
        "extra": false
      },
      "squeeze_more": false,
      "pretty": false,
      "separator": ";"
    }
  },

  "modules": [{
    "name": "app",
    "in": "main.js",
    "out": "app.js"
  }]
}